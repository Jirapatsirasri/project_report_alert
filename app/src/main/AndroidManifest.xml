<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asus.project_alert">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_alert"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_alert_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".LoginActivity" />
        <activity
            android:name=".report"
            android:parentActivityName=".MainUser" />
        <activity android:name=".complete_alert" />
        <activity android:name=".MainUser" />
        <activity
            android:name=".showAlert"
            android:parentActivityName=".MainUser" />
        <activity android:name=".Alert" />
        <activity
            android:name=".CallAlert"
            android:parentActivityName=".MainUser" />
        <activity android:name=".ProfileUser" android:parentActivityName=".MainUser" />
        <activity android:name=".EditProfile" android:parentActivityName=".ProfileUser"></activity>
    </application>

</manifest>